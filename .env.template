# Required Configuration
CLAUDE_API_KEY=your_claude_api_key_here
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/university_scraper

# Optional Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Scraper Settings
SCRAPER_DELAY_MIN=2
